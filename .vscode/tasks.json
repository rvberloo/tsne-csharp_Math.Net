{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/tsne-csharp_Math.Net.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-release",
            "type": "shell",
            "command": "dotnet",
            "args": [
                "build",
                "--configuration",
                "Release",
                "${workspaceFolder}/tsne-csharp_Math.Net.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}